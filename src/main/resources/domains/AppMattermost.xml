<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="studio" package="com.axelor.studio.db"/>

  <entity name="AppMattermost" cacheable="true">

    <one-to-one ref="com.axelor.studio.db.App" name="app"/>
    <string name="mattermostUrl" title="Url mattermost"/>
    <string name="mattermostToken" password="true" title="Token mattermost"/>
    <string name="tenantName" title="Tenant name"/>
    <string name="teamId" title="Team id"/>
  </entity>

</domain-models>
